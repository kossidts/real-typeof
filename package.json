{
    "name": "@kdts/real-typeof",
    "version": "1.1.1",
    "description": "Retrieve the real type of a variable, a more accurate version of the `typeof` operator. This module has been improved for performance and is getting better overtime.",
    "main": "./src/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./types/wrapper.d.mts",
                "default": "./src/wrapper.mjs"
            },
            "require": {
                "types": "./types/index.d.ts",
                "default": "./src/index.js"
            }
        }
    },
    "files": [
        "./src/index.js",
        "./src/wrapper.mjs",
        "./types/index.d.ts",
        "./types/wrapper.d.mts"
    ],
    "type": "commonjs",
    "scripts": {
        "benchmark": "node benchmark",
        "build:type": "tsc --build --force"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kossidts/real-typeof.git"
    },
    "homepage": "https://github.com/kossidts/real-typeof#readme",
    "bugs": {
        "url": "https://github.com/kossidts/real-typeof/issues"
    },
    "keywords": [
        "typeof",
        "type of",
        "real typeof",
        "real-typeof",
        "real type of",
        "real-type-of",
        "real_type_of",
        "deep typeof",
        "isNumber"
    ],
    "author": "Kossi D. T. Saka",
    "license": "MIT",
    "devDependencies": {
        "benchmark": "^2.1.4",
        "typescript": "^5.4.3"
    }
}
